{
	"extends": "@tsconfig/node-lts/tsconfig.json",
	"compilerOptions": {
		"module": "NodeNext",
		"resolveJsonModule": true,
		"outDir": "./out/ts/",
		"allowJs": false,
		"sourceMap": true,
		"useUnknownInCatchVariables": false,
		"strictNullChecks": false,
	},
	"exclude": ["node_modules", "out", "electron/commonjs/", "android", "www"],
}